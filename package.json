{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "val-town",
  "title": "Val Town",
  "description": "A simple Raycast extension to fetch information about Vals from val.town",
  "icon": "command-icon.png",
  "author": "nbbaier",
  "license": "MIT",
  "commands": [
    {
      "name": "fetchByName",
      "title": "Fetch Val by Name",
      "description": "Get a val by it's name",
      "arguments": [
        {
          "name": "user",
          "type": "text",
          "placeholder": "user",
          "required": true
        },
        {
          "name": "valname",
          "type": "text",
          "placeholder": "val name",
          "required": true
        }
      ],
      "mode": "view"
    },
    {
      "name": "fetchByID",
      "title": "Fetch Val by ID",
      "description": "Get a val by it's name",
      "arguments": [
        {
          "name": "valid",
          "type": "text",
          "placeholder": "val id",
          "required": true
        }
      ],
      "mode": "view"
    },
    {
      "name": "createVal",
      "title": "Create a Val",
      "description": "Get a val by it's name",
      "mode": "no-view"
    }
  ],
  "preferences": [
    {
      "name": "apiToken",
      "type": "password",
      "description": "API Token for val.town",
      "title": "API Token",
      "placeholder": "API Token",
      "required": true
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.55.2",
    "@raycast/utils": "^1.8.0",
    "zx": "^7.2.3"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}